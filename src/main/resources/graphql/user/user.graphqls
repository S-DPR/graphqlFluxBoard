extend type Query {
    users: [User]
    user(userId: ID!): User
}

extend type Mutation {
    createUser(saveUserInput: SaveUserInput!): User
    deleteUser(deleteUserInput: DeleteUserInput!): Boolean
}

type User {
    id: ID!
    username: String!
}

input SaveUserInput {
    username: String!
    password: String!
}

input DeleteUserInput {
    userId: ID!
    password: String!
}

