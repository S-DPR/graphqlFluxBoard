type Post {
    id: ID!
    title: String
    user: User
    content: String
    createdAt: String
}

input PostInput {
    title: String
    content: String
    password: String
    userId: String!
}

input PostFilterInput {
    type: FilterType
    keyword: String
    page: Int
    sizePerPage: Int
    sortField: String
    sortOrder: SortOrder
}

enum FilterType {
    NONE
    AUTHOR
    TITLE
    CONTENT
    TITLE_AND_CONTENT
}

enum SortOrder {
    ASC
    DESC
}

